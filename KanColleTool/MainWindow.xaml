<Window x:Class="KanColleTool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ネオアームストロングサイクロンジェットアームストロング砲" Height="414" Width="470" Closing="Window_Closing">

    <Grid>
        <DockPanel Margin="0,36,0,0" VerticalAlignment="Top" HorizontalAlignment="Left"
                   Width="{Binding ActualWidth, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type Grid}}}">
            <Frame Name="mainFrame" NavigationUIVisibility="Hidden" />
        </DockPanel>
        <Button Content="DashBoard" x:Uid="DashBoard" Height="30" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Click="Button_Click" />
        <Button Content="Ship" x:Uid="ShipListPage" Height="30" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Click="Button_Click" Margin="74,0,0,0" />
        <Button Content="Item" x:Uid="ItemListPage" Height="30" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Click="Button_Click" Margin="148,0,0,0" />
        <Button Content="Spec" x:Uid="SpecListPage" Height="30" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Click="Button_Click" Margin="222,0,0,0" />
        <Button Content="Quest" x:Uid="QuestListPage" Height="30" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Click="Button_Click" Margin="296,0,0,0" />
    </Grid>
</Window>
